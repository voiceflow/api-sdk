{
  "name": "@voiceflow/api-sdk",
  "description": "wrapper for creator-api",
  "version": "0.1.0",
  "author": "Voiceflow",
  "bugs": {
    "url": "https://github.com/voiceflow/api-sdk/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/atob": "^2.1.2",
    "@voiceflow/logger": "^1.1.10",
    "atob": "^2.1.2",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "io-ts": "^2.2.8",
    "regenerator-runtime": "^0.13.5",
    "superstruct": "^0.10.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.0",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "@voiceflow/commitlint-config": "^1.0.1",
    "@voiceflow/eslint-config": "^2.0.4",
    "@voiceflow/git-branch-check": "^1.1.3",
    "@voiceflow/prettier-config": "^1.0.4",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codacy-coverage": "^3.4.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.4.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "fixpack": "^3.0.6",
    "husky": "^4.2.5",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.1.1",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.19",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.6"
  },
  "homepage": "https://github.com/voiceflow/api-sdk#readme",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "__pre-commit": "lint-staged",
      "pre-push": "git-branch-check"
    }
  },
  "license": "ISC",
  "lint-staged": {
    "package.json": [
      "fixpack",
      "git add"
    ],
    "**/*.{js,ts}": [
      "eslint --fix",
      "prettier-eslint --write",
      "git add"
    ]
  },
  "prettier": "@voiceflow/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/api-sdk.git"
  },
  "scripts": {
    "build": "npm run clean && ttsc --project ./tsconfig.build.json",
    "clean": "rimraf build",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "test": "NODE_ENV=test nyc ts-mocha --paths --config ./config/tests/.mocharc.json tests/**/*.{unit,it}.ts",
    "test:integration": "NODE_ENV=test nyc ts-mocha --paths --opts ./config/tests/.mocharc.json tests/**/*.it.ts",
    "test:single": "NODE_ENV=test ts-mocha --paths --config ./config/tests/.mocharc.json",
    "test:unit": "NODE_ENV=test nyc ts-mocha --paths --config ./config/tests/.mocharc.json tests/**/*.unit.ts",
    "tdd": "yarn test --watch",
    "prepare": "npm run build"
  }
}
